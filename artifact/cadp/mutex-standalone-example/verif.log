"svl001_formula_inline.mcl" =
    macro is_in (t, f) =
      index(f, concat(concat(":", string(t)), ":")) <> 0
    end_macro
    < true* . { STEP ... ?F:String }  >
      <
        for tid:Nat from 0 to 2 do
          if is_in(tid, F) then
            true* . { STEP !tid ... }
          end if
        end for
      >@

